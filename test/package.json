		"test": "node ./node_modules/vscode/bin/test"
	},
	"devDependencies": {
		"@types/node": "^8.0.47",
		"vscode": "^1.1.4"
	},
	"dependencies": {
		"babel-core": "^6.26.0",
		"gulp": "^3.9.1"
